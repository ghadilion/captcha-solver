{
    "manifest_version": 3,
    "name": "Captcha Solver",
    "description": "Amazon Captcha solver",
    "version": "1.0",
    "icons": {
        "16": "./icons/ext-icon-orig-16.png",
        "32": "./icons/ext-icon-orig-32.png",
        "48": "./icons/ext-icon-orig-48.png",
        "128": "./icons/ext-icon-orig-128.png"
    },
    "content_scripts": [
        {
            "js": ["client.js"],
            "matches": ["http://127.0.0.1:5000/*"]
        }
    ]
}
